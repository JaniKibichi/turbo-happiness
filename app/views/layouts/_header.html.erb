 <header id="main_header" class="cf">
 <div class="wrapper_wide">
 <%= link_to "Toa Mpango", root_path, id:"logo" %>

  <nav>
    <ul>
    <% unless user_signed_in? %>
    <li><%= link_to "Places", posts_path %></li>
    <li><%= link_to "Share a place", new_user_registration_path %></li>
    <li><%= link_to "Sign In", new_user_session_path %></li>
    <% else %>
    <li><%= link_to current_user.name, edit_user_registration_path %></li>
    <li><%= link_to "Places", posts_path %></li>
    <li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
    <% end %>
   </ul>

 </nav>
 </div>
 </header>
